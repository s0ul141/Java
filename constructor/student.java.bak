class student
{	int std_id;
	String std_name;
	char std_grade
	student(int x, String y, char z)
	{
		std_id=x;
		std_name=y;
		std_grade=z;
	}

	public static void main(String[]args)
	{
		student s1 = new student(1,"soumya",'A');
		System.out.println(s1.std_id);
		System.out.println(s1.std_name);
		System.out.println(s1.std_grade);
	}
}